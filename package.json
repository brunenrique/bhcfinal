{
  "name": "thalamus",
  "description": "Plataforma web para gestão de clínicas de psicologia, com agenda integrada, prontuários seguros e funcionalidades auxiliadas por IA.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 9003",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "dev:force": "lsof -ti:9003 | xargs kill -9 || true && sleep 2 && next dev --turbopack -p 9003",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "scripts/run-tests.sh",
    "test:rules": "firebase emulators:exec --project=demo-project \"npx jest --selectProjects firestore\"",
    "jest": "jest",
    "backup": "node scripts/backup.js",
    "prepare": "husky install"
  },
